{
  "manifest_version": 2,
  "name": "Link Nuker",
  "version": "1.0.0",
  "description": "Turns selected link into plain text and plays a victorious gif!",
  "icons": {
    "128": "./icons/icon128.png",
    "64": "./icons/icon64.png",
    "16": "./icons/icon16.png"
  },
  "browser_action": {
    "default_title": "Link Nuker",
    "default_icon": "./icons/icon16.png",
    "default_popup": "extension-ui.html"
  }
}
